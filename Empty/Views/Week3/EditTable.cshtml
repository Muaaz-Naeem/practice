@model IList< Empty.Models.Employee>

@{
    ViewBag.Title = "EditTable";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>EditTable</h2>

<div>
    <h4>Employee</h4>
    @for (int i = 0; i < Model.Count(); i++)
    {
        <h2>Student @i</h2>
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => Model[i].Name)
            </dt>

            <dd>
                @Html.DisplayFor(model => Model[i].Name)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => Model[i].Address)
            </dt>

            <dd>
                @Html.DisplayFor(model => Model[i].Address)
            </dd>

        </dl>
        <hr />

    }

</div>
 
